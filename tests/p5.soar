# Simple chunk test

package require Soar;

sp {propose*left
    (state <s> ^superstate nil)
    -->
    (<s> ^operator <o> +)
    (<o> ^name left)}

sp {propose*right
    (state <s> ^superstate nil)
    -->
    (<s> ^operator <o> +)
    (<o> ^name right)}


sp {prefer*left
    (state <s> ^impasse tie ^attribute operator ^superstate <ss>)
    (<ss> ^operator <o1> + ^operator <o2> +)
    (<o1> ^name left)
    (<o2> ^name right)
    -->
    (<ss> ^operator <o1> > <o2>)}

